{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:recommended",
    ":prHourlyLimitNone",
    ":prConcurrentLimitNone",
    ":disableDependencyDashboard",
    ":semanticCommits",
    ":enablePreCommit"
  ],
  "forkProcessing": "enabled",
  "bbUseDefaultReviewers": true,
  "baseBranches": [
    "main"
  ]
}
